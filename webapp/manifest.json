{
	"_version": "1.4.0",
	"sap.app": {
		"id": "com.vinci.timesheet.admin",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZHR_MOB_TIMESHEET_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.1worklist",
			"version": "1.46.8"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize", "sap_belize_plus"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.vinci.timesheet.admin.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.42.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.42.0"
				},
				"sap.m": {
					"minVersion": "1.42.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.vinci.timesheet.admin.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"defaultCountMode": "Inline"
				}
			},
			"user": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "/services/userapi/currentUser",
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.vinci.timesheet.admin.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": ["notFound"]
				},
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "home",
				"target": ["home"]
			}, {
				"name": "periodSelection",
				"pattern": "periodSelection",
				"titleTarget": "",
				"greedy": false,
				"target": ["periodSelection"]
			}, {
				"name": "AddTimesheet",
				"pattern": "AddTimesheet",
				"titleTarget": "",
				"greedy": false,
				"target": ["AddTimesheet"]
			}, {
				"name": "ReportEmployeeSelection",
				"pattern": "ReportEmployeeSelection",
				"titleTarget": "",
				"greedy": false,
				"target": ["ReportEmployeeSelection"]
			}, {
				"name": "WeeklyReport",
				"pattern": "WeeklyReport",
				"titleTarget": "",
				"greedy": false,
				"target": ["WeeklyReport"]
			}],
			"targets": {
				"home": {
					"viewName": "WeeklySummary",
					"viewId": "WeeklySummary",
					"viewLevel": 1,
					"title": "{i18n>worklistViewTitle}"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				},
				"periodSelection": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "TimesheetSelection",
					"viewLevel": 2,
					"viewId": "TimesheetSelection"
				},
				"AddTimesheet": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "AddTimesheet",
					"viewLevel": 3,
					"viewId": "AddTimesheet"
				},
				"ReportEmployeeSelection": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ReportEmployeeSelection",
					"viewId": "ReportEmployeeSelection",
					"viewLevel": 2
				},
				"WeeklyReport": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "WeeklyReport",
					"viewId": "WeeklyReport",
					"viewLevel": 2
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css",
				"id": ""
			}],
			"js": [{
				"uri": "utility/tableHeadFixer"
			}]
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}